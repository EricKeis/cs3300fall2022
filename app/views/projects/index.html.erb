<div class="container">
  <p style="color: green"><%= notice %></p>

  <strong><h1 class="display-2 mb-5">Projects</h1></strong>

  <div id="projects">
    <% @projects.each do |project| %>
      <%= render project %>
      <p>
        <%= link_to "Show this project", project, :class => "btn btn-info btn-sm me-1" %>
      </p>
    <% end %>
  </div>

  <%= link_to "New project", new_project_path, :class => "btn btn-success me-1 mt-3" %>
</div>